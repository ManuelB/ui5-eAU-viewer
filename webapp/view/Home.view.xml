<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="myhealthapp.controller.Home"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>title}">
		<content>
			<HBox alignItems="block">
				<GenericTile id="myPatientsTile" binding="{/Patient}" class="sapUiTinyMarginBegin sapUiTinyMarginTop" header=" Ansicht Versicherter" press="eauPress">
				</GenericTile>
				<GenericTile id="AGTile"  class="sapUiTinyMarginBegin sapUiTinyMarginTop" header=" Ansicht Arbeitgeber" press="onAGPress">
				</GenericTile>
				<GenericTile id="KrKaTile"  class="sapUiTinyMarginBegin sapUiTinyMarginTop" header=" Ansicht Krankenkasse" press="onKrKaPress">
				</GenericTile>
				<GenericTile id="StornoTile"  class="sapUiTinyMarginBegin sapUiTinyMarginTop" header=" Ansicht Stornierung" press="onStornoPress">
				</GenericTile>
				<Image src="" width="{300}" />
			</HBox>
			<VBox id="bx2">
				<Text text="" class="sapUiSmallMarginBottom" />
				<Text text="" class="sapUiSmallMarginBottom" />
				<Image src="" width="{300}" />
			</VBox>
		</content>
		<footer>
			<OverflowToolbar id="overflowToolbar">
            	<Label text="{i18n>homeFooterText}"/>				
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>